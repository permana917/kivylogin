#:import PageMain pages.pagemain.pagemain

<PageMain>:
    name: "main_screen"
    StackLayout:
        orientation: "lr-tb"
        BoxLayout:
            orientation: "vertical"
            size_hint_y: .5

            MDLabel:
                text: "This is Main Screen of Yours"
                size_hint: 1, 1
                valign:'middle'

            MDRaisedButton:
                text: "Logout"
                size_hint: None, None
                pos_hint: {"center_x": .5}
                on_release: root.logout()